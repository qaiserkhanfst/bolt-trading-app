{
  "name": "ai-crypto-trading-platform",
  "version": "1.0.0",
  "description": "AI-powered cryptocurrency trading platform",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-client": "cd client && npm install",
    "build-client": "cd client && npm run build",
    "postinstall": "npm run install-client"
  },
  "keywords": [
    "crypto",
    "trading",
    "ai",
    "openai",
    "binance"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "binance-api-node": "^0.12.6",
    "chart.js": "^4.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase": "^10.6.0",
    "firebase-admin": "^11.11.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "node-binance-api": "^0.13.1",
    "node-cache": "^5.1.2",
    "openai": "^4.20.1",
    "socket.io": "^4.7.2",
    "technicalindicators": "^3.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
